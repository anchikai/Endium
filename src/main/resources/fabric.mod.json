{
	"schemaVersion": 1,
	"id": "endium",
	"version": "${version}",
	"name": "The Endium Expansion",
	"description": "This is a mod that adds an alternative to Netherite with its own unique properties as well as more materials to The End.",
	"authors": [
		"anchikai."
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/endium",
		"sources": "https://github.com/anchikai/Endium",
		"issues": "https://github.com/anchikai/Endium/issues"
	},
	"license": "MIT",
	"icon": "assets/endium/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.anchikai.endium.EndiumMod"
		],
		"client": [
          "net.anchikai.endium.EndiumModClient"
		],
		"fabric-datagen": [
			"net.anchikai.endium.EndiumModDataGenerator"
		]
	},
	"mixins": [
        "endium.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.12",
		"minecraft": "~1.19.4",
		"java": ">=17",
		"fabric-api": "*",
		"reach-entity-attributes": ">=2.3.2",
		"dawn": ">=4.2.1"
	},
	"suggests": {
		"forlorn": "*"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.twitter": "https://twitter.com/anchikai_",
				"modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/anchikais-endium",
				"modmenu.modrinth": "https://modrinth.com/mod/endium",
				"modmenu.github_releases": "https://github.com/anchikai/Endium/releases"
			},
			"modupdater": {
				"strategy": "github",
				"owner": "anchikai",
				"repository": "Endium"
			}
		}
	}
}